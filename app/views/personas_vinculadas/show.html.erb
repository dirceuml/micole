<div id='persona_vinculada' data-role="header">
  <h1>Detalles</h1>
  <%= link_to 'Aceptar', personas_vinculadas_path, "data-icon" => "check", "data-mini" => "true"  %>
</div>

<div data-role="content">
  <div class="field"><b>Documento:</b> <%= "#{ListaValor.find_by_tabla_and_item(2, @persona_vinculada.tipo_documento).abreviatura} #{@persona_vinculada.numero_documento}" %><p></div>
  <div class="field"><b>Apellidos y nombres:</b> <%= @persona_vinculada.apellidos_nombres %><p></div>
  <div class="field"><b>Telefono fijo:</b> <%= @persona_vinculada.telefono_fijo %><p></div>
  <div class="field"><b>Telefono m√≥vil:</b> <%= @persona_vinculada.telefono_movil %><p></div>
  <div class="field"><b>Correo:</b> <%= @persona_vinculada.correo %><p></div>
  <div class="field"><b>Foto:</b> <%= @persona_vinculada.foto %><p></div>
</div>

<div data-role="footer">
  <%= link_to 'Editar', edit_persona_vinculada_path(@persona_vinculada), "class" => "ui-btn-left", "data-inline" => "true", "data-theme" => "a", "data-icon" => "gear", "data-mini" => "true" %>
</div>
