<% @anios_escolares.each do |anio_escolar| %>
  <tr>
    <td><%= anio_escolar.colegio_id %></td>
    <td><%= anio_escolar.anio %></td>
    <td><%= anio_escolar.inicio_clases %></td>
    <td><%= anio_escolar.fin_clases %></td>
    <td><%= anio_escolar.usuario %></td>
    <td><%= link_to 'Show', anio_escolar %></td>
    <td><%= link_to 'Edit', edit_anio_escolar_path(anio_escolar) %></td>
    <td><%= link_to 'Destroy', anio_escolar, confirm: 'Are you sure?', method: :delete %></td>
