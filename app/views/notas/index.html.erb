<h1>Listing notas</h1>

<table>
  <tr>
    <th>Anio alumno</th>
    <th>Curso</th>
    <th>Tipo nota</th>
    <th>Nota</th>
    <th>Observaciones</th>
    <th>Usuario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notas.each do |nota| %>
  <tr>
    <td><%= nota.anio_alumno_id %></td>
    <td><%= nota.curso_id %></td>
    <td><%= nota.tipo_nota_id %></td>
    <td><%= nota.nota %></td>
    <td><%= nota.observaciones %></td>
    <td><%= nota.usuario %></td>
    <td><%= link_to 'Show', nota %></td>
    <td><%= link_to 'Edit', edit_nota_path(nota) %></td>
    <td><%= link_to 'Destroy', nota, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Nota', new_nota_path %>
